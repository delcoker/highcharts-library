{
  "name": "highcharts-library",
  "version": "1.0.0",
  "description": "High Chart Switcher",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delcoker/highcharts-library.git"
  },
  "keywords": [
    "highcharts",
    "charts"
  ],
  "author": "delcoker",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/delcoker/highcharts-library/issues"
  },
  "homepage": "https://github.com/delcoker/highcharts-library#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.41",
    "builder-pattern": "^2.2.0",
    "class-transformer": "^0.5.1",
    "codecov": "^3.1.0",
    "eslint": "^8.0.1",
    "jest": "^29.5.0",
    "prettier": "^2.4.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "files": [
    "lib/**/*"
  ]
}
