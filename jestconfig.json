{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "coverageDirectory": "./coverage/",
  "collectCoverage": true,
  "esModuleInterop": true,
  "resolver": "jest-ts-webcompat-resolver"
}
